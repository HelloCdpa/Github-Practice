package java1018;

public class Trainee {
	
	String name, age, phonenum;
	int Major;
	
	Trainee (String name, String age, String phonenum, int Major) {
		this.name = name;
		this.age = age;
		this.Major = Major;
		this.phonenum = phonenum;
	}
	
	void afterSignup (String name, int Major){
		this.name = name;
		this.Major = Major;
		
		System.out.println("등록 완료");
		
	}
	void phoneNumberUpdate (String phonenum) {
		this.phonenum = phonenum;
		System.out.println("변경 완료");
		
	}
	
	
	

	
	
	
	
	
	
	

}
